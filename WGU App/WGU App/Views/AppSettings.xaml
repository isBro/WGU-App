<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WGU_App.Views.AppSettings">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="LoadSampleData" Text="Load Sample Data" VerticalOptions="Center" HorizontalOptions="Center" WidthRequest="150" Clicked="LoadSampleData_Clicked"></Button>
            <Button x:Name="ClearSampleData" Text="Clear Sample Data" VerticalOptions="Center" HorizontalOptions="Center" WidthRequest="150" Clicked="ClearSampleData_Clicked"></Button>
            <Button x:Name="ClearPreferences" Text="Clear Preferences" VerticalOptions="Center" HorizontalOptions="Center" WidthRequest="150" Clicked="ClearPreferences_Clicked"></Button>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>